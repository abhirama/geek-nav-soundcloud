{
  "name": "SoundCloudNav",
  "version": "0.1",
  "description": "A keyboard based track navigator for SoundCloud.",
  "permissions": [
    "tabs"
  ],
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["listener.js"]
    },
    {
      "matches": ["*://soundcloud.com/*"],
      "js": ["soundcloud.js"]
    }
  ]
}




